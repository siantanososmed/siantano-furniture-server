{
  "kind": "collectionType",
  "collectionName": "product_colors",
  "info": {
    "singularName": "product-color",
    "pluralName": "product-colors",
    "displayName": "ProductColor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "private": true,
      "required": true,
      "unique": true
    },
    "productMedia": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "product_colors"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_colors"
    }
  }
}
